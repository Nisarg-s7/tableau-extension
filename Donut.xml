<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" xmlns="http://www.tableau.com/xml/extension_manifest">
  
  <worksheet-extension id="com.tableau.extension.demo.bar" extension-version="1.0.0">
    
    <default-locale>en_US</default-locale>
    
    <name resource-id="name"/>
    <description>Donut Chart Extension</description>
    
    <author name="You" email="you@email.com" organization="YourOrg" website="https://example.com"/>
    
    <min-api-version>1.11</min-api-version>
    
    <source-location>
      <url>http://localhost:8000/donut.html</url>
    </source-location>

    <icon/>

    <!-- CATEGORY ENCODING (matches JavaScript code) -->
    <encoding id="Dimension">
      <display-name>Dimension</display-name>
      <role-spec>
        <role-type>discrete-dimension</role-type>
      </role-spec>
      <fields max-count="1"/>
    </encoding>

    <!-- MEASURE ENCODING -->
    <encoding id="measure">
      <display-name>Measure</display-name>
      <role-spec>
        <role-type>continuous-measure</role-type>
      </role-spec>
      <fields max-count="1"/>
    </encoding>

  </worksheet-extension>

  <resources>
    <resource id="name">
      <text locale="en_US">Pie Chart</text>
    </resource>
  </resources>

</manifest>
